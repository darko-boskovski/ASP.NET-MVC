
@model ProjectViewModel


<h3>Entered Project Details</h3>


<ul>
    <li>EProject Name:  @Model.Name</li>
    <li>Project Start Date:  @Model.StartDate</li>
    <li>Project End Date:  @Model.EndDate</li>
    <li>Employee Name:  @Model.Client.FullName</li>


    @for (int i = 0; i < Model.Employees.Count; i++)
    {

        <ul>
            <li>Employee Name:  @Model.Employees[i].FullName</li>
            <li>Employee Email Address:  @Model.Employees[i].EmailAddress</li>

        </ul>

    }
</ul>


